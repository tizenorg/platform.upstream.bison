msgid "syntax error"
msgstr "構文エラー"

#: data/glr.c:2078 data/lalr1.cc:942 data/yacc.c:1347
#, c-format
msgid "syntax error, unexpected %s"
msgstr "構文エラー、予期しない %s です"

